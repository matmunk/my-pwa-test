{
    "generalConfiguration.sortOnBeforeSave": true,
    "importSorter.sortConfiguration.importPaths.order": "caseInsensitive",
    "importSorter.sortConfiguration.joinImportPaths": false,
    "importStringConfiguration.maximumNumberOfImportExpressionsPerLine.type": "newLineEachExpressionAfterCountLimitExceptIfOnlyOne",
    "importStringConfiguration.maximumNumberOfImportExpressionsPerLine.count": 130,
    "importSorter.importStringConfiguration.spacingPerImportExpression.afterComma": 1,
    "importSorter.sortConfiguration.customOrderingRules.defaultNumberOfEmptyLinesAfterGroup": 0,
    "importSorter.importStringConfiguration.tabType": "space",
    "importSorter.importStringConfiguration.trailingComma": "multiLine",
    "importStringConfiguration.tabSize": 4,
    "importStringConfiguration.quoteMark": "double",
    "importSorter.importStringConfiguration.hasSemicolon": false,
    "importSorter.sortConfiguration.removeUnusedImports": true,
    "importSorter.sortConfiguration.removeUnusedDefaultImports": true,
    "importSorter.generalConfiguration.sortOnBeforeSave": true,
    "importSorter.importStringConfiguration.quoteMark": "double",
    "importSorter.sortConfiguration.customOrderingRules.rules": [
        {
            "type": "importMember",
            "regex": "^$",
            "orderLevel": 1,
            "disableSort": true
        },
        {
            "regex": "\\.styl",
            "orderLevel": 9
        },
        {
            "regex": "^react",
            "orderLevel": 10
        },
        {
            "regex": "^@mui",
            "orderLevel": 11
        },
        {
            "regex": "^[@]",
            "orderLevel": 12
        },
        {
            "regex": "^((?!/).)*$",
            "orderLevel": 13
        },
        {
            "regex": "^hooks",
            "orderLevel": 20
        },
        {
            "regex": "^server|^utils",
            "orderLevel": 30
        },
        {
            "regex": "^models",
            "orderLevel": 40
        },
        {
            "regex": "^pages",
            "orderLevel": 50
        },
        {
            "regex": "^controls",
            "orderLevel": 60
        },
        {
            "regex": "^common",
            "orderLevel": 70
        },
        {
            "regex": "^./*",
            "orderLevel": 80
        },
        {
            "regex": "^../*",
            "orderLevel": 90
        }
    ]
}